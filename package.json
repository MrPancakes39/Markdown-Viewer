{
  "name": "markdown-viewer",
  "version": "1.1.1",
  "description": "A Simple Markdown Viewer for rest of us.",
  "main": "index.js",
  "repository": "https://github.com/MrPancakes39/Markdown-Viewer",
  "scripts": {
    "start": "electron .",
    "test": "electron --version",
    "gen-tmp": "node generate-template.js",
    "package-win": "electron-packager . markdown-viewer --overwrite --asar=true --platform=win32 --arch=ia32 --icon=assets/icon.ico --prune=true --out=release-builds --version-string.CompanyName=SH --version-string.FileDescription=SH --version-string.ProductName=\"Markdown Viewer\"",
    "package-linux": "electron-packager . markdown-viewer --overwrite --asar=true --platform=linux --arch=x64 --icon=assets/icon.png --prune=true --out=release-builds"
  },
  "keywords": [
    "markdown"
  ],
  "author": "Salman Hasan <salmannhassan39@gmail.com>",
  "license": "GPL-3.0",
  "private": false,
  "dependencies": {
    "temp": "^0.9.4"
  },
  "devDependencies": {
    "electron": "^13.1.6",
    "electron-packager": "^15.2.0"
  }
}